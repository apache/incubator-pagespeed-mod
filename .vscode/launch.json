{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Test",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "",
            "program": "${workspaceRoot}/bazel-bin/pagespeed/kernel/base/pagespeed_base_core_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "TEST_TMPDIR",
                    "value": "/tmp/foo"
                },
                {
                    "name": "TEST_WORKSPACE",
                    "value": "."
                },
                {
                    "name": "TEST_SRCDIR",
                    "value": "${workspaceRoot}"
                },
                {
                    "name": "ENVOY_IP_TEST_VERSIONS",
                    "value": ""
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "sourceFileMap": {
                "/proc/self/cwd/external/envoy": "${workspaceFolder}/nighthawk/envoy"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}